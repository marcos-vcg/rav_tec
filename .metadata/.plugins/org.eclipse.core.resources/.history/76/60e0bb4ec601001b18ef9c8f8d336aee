import javax.swing.JInternalFrame;

public class Cliente extends JInternalFrame {
	static final int xPosition = 30, yPosition = 30;

	public Cliente() {
		super("Cadastro de Clientes", true, // resizable
				true, // closable
				true, // maximizable
				true);// iconifiable
		setSize(300, 300);
		setLocation(xPosition, yPosition);
	}
}
